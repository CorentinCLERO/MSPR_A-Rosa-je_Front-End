{
  "cli": {
    "version": ">= 5.9.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://upward-splendid-ghoul.ngrok-free.app"
      }
    },
    "production": {
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://upward-splendid-ghoul.ngrok-free.app"
      }
    },
    "test": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://upward-splendid-ghoul.ngrok-free.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}