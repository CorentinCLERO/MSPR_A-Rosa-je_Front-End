{
  "expo": {
    "name": "mspr-arosaje-frontend",
    "slug": "mspr-arosaje-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logos/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logos/logo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow maps to use your location."
        }
      ]
    ]
  }
}
